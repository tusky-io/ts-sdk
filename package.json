{
  "name": "@akord/carmella-sdk",
  "version": "0.9.0",
  "description": "A set of tools designed for integrating file and folder uploads into secure digital vaults that are stored on-chain with Akord",
  "main": "lib/index.js",
  "repository": "git@github.com:Akord-com/carmella-sdk.git",
  "scripts": {
    "test": "jest --verbose --collectCoverage=false --forceExit --detectOpenHandles",
    "build": "rm -r lib || true && tsc -p .",
    "co:login": "aws codeartifact login --tool npm --repository carmella --domain akord --region eu-central-1"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/index.d.ts"
  ],
  "keywords": [
    "akord",
    "storage",
    "web3",
    "encryption",
    "e2ee"
  ],
  "bugs": {
