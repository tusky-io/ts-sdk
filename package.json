{
  "name": "@akord/carmella-sdk",
  "version": "0.22.0-experimental.3",
  "description": "A set of tools designed for integrating file and folder uploads into secure digital vaults that are stored on-chain with Akord",
  "main": "./lib/web/cjs/index.js",
  "module": "./lib/web/esm/index.esm.js",
  "types": "./lib/web/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/web/cjs/index.js",
      "import": "./lib/web/esm/index.esm.js",
      "default": "./lib/web/cjs/index.js",
      "types": "./lib/web/types/index.d.ts"
    },
    "./node": {
      "require": "./lib/node/cjs/index.js",
      "import": "./lib/node/esm/index.esm.js",
      "default": "./lib/node/cjs/index.js",
      "types": "./lib/node/types/index.d.ts"
    },
    "./web": {
      "require": "./lib/web/cjs/index.js",
      "import": "./lib/web/esm/index.esm.js",
      "default": "./lib/web/cjs/index.js",
      "types": "./lib/web/types/index.d.ts"
    }
  },
  "repository": "git@github.com:Akord-com/carmella-sdk.git",
  "scripts": {
    "test": "npm run test:user && npm run test:vault && npm run test:vault:private",
    "test:user": "jest --verbose --collectCoverage=false --forceExit --detectOpenHandles src/__tests__/user/*.test.ts",
    "test:vault": "jest --verbose --collectCoverage=false --forceExit --detectOpenHandles src/__tests__/vault/*.test.ts",
    "test:vault:private": "jest --verbose --collectCoverage=false --forceExit --detectOpenHandles src/__tests__/vault/*.test.ts -- --encrypted",
    "build": "webpack --config webpack.config.js",
    "analyze": "STATS=server npm run build",
    "set:registry": "bash ./registry.sh",
    "co:login": "aws codeartifact login --tool npm --repository carmella --domain akord --region eu-central-1"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/web/types/index.d.ts",
    "lib/node/types/index.d.ts"
  ],
  "keywords": [
    "akord",
    "storage",
    "web3",
    "encryption",
    "e2ee"
  ],
  "bugs": {
    "url": "https://github.com/Akord-com/carmella-sdk/issues"
  },
  "homepage": "https://github.com/Akord-com/carmella-sdk#readme",
  "author": "Akord Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@akord/browser-level": "^1.0.2",
    "@akord/carmella-gql": "^0.48.1",
    "@akord/ts-cacheable": "1.0.13",
    "@aws-amplify/api": "^6.0.54",
    "@aws-amplify/api-graphql": "^4.4.1",
    "@aws-amplify/core": "^6.4.5",
    "@esm2cjs/p-queue": "^7.3.0",
    "@uppy/core": "^4.2.0",
    "@uppy/tus": "^4.1.0",
    "agentkeepalive": "^4.5.0",
    "axios": "0.27.2",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "cross-fetch": "^4.0.0",
    "ethereum-cryptography": "^1.2.0",
    "jssha": "^3.3.1",
    "libsodium-wrappers": "^0.7.15",
    "memory-level": "^1.0.0",
    "memorystorage": "^0.12.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "tus-js-client": "^4.2.3",
    "tweetnacl": "^1.0.3",
    "uuid": "^8.3.2",
    "web-streams-polyfill": "^3.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@mysten/sui": "^1.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/lodash": "^4.14.202",
    "@types/node": "^17.0.8",
    "@types/pngjs": "^6.0.5",
    "@types/readline-sync": "^1.4.8",
    "@types/streamsaver": "^2.0.1",
    "@types/uuid": "^8.3.4",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv": "^16.0.2",
    "express": "^4.19.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "google-auth-library": "^9.12.0",
    "jasmine-core": "^5.3.0",
    "jest": "^29.2.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-webpack": "^5.0.1",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "pngjs": "^7.0.0",
    "readline-sync": "^1.4.10",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.0",
    "ts-loader": "9.3.1",
    "ts-patch": "^3.2.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.1.6",
    "typescript-transform-paths": "^3.5.1",
    "webpack": "^5.94.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
