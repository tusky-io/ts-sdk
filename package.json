{
  "name": "@akord/carmella-sdk",
  "version": "0.21.0-dev.2",
  "description": "A set of tools designed for integrating file and folder uploads into secure digital vaults that are stored on-chain with Akord",
  "main": "lib/bundle.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:Akord-com/carmella-sdk.git",
  "scripts": {
    "test": "jest --verbose --collectCoverage=false --forceExit --detectOpenHandles",
    "build:dev": "rm -r lib || true && tsc -p .",
    "build": "webpack --mode production",
    "set:registry": "bash ./registry.sh",
    "co:login": "aws codeartifact login --tool npm --repository carmella --domain akord --region eu-central-1"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/index.d.ts"
  ],
  "keywords": [
    "akord",
    "storage",
    "web3",
    "encryption",
    "e2ee"
  ],
  "bugs": {
    "url": "https://github.com/Akord-com/carmella-sdk/issues"
  },
  "homepage": "https://github.com/Akord-com/carmella-sdk#readme",
  "author": "Akord Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@akord/browser-level": "^1.0.2",
    "@akord/ts-cacheable": "1.0.13",
    "@esm2cjs/p-queue": "^7.3.0",
    "agentkeepalive": "^4.5.0",
    "axios": "0.27.2",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "cross-fetch": "^4.0.0",
    "ethereum-cryptography": "^1.2.0",
    "form-data": "^4.0.0",
    "jssha": "^3.3.1",
    "libsodium-wrappers": "^0.7.15",
    "lodash": "^4.17.21",
    "memory-level": "^1.0.0",
    "memorystorage": "^0.12.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "tus-js-client": "^4.1.0",
    "tweetnacl": "^1.0.3",
    "uuid": "^8.3.2",
    "web-streams-polyfill": "^3.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@mysten/sui": "^1.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/lodash": "^4.14.202",
    "@types/node": "^17.0.8",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/readline-sync": "^1.4.8",
    "@types/streamsaver": "^2.0.1",
    "@types/uuid": "^8.3.4",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv": "^16.0.2",
    "express": "^4.19.2",
    "google-auth-library": "^9.12.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "pngjs": "^7.0.0",
    "readline-sync": "^1.4.10",
